@import '../../styles/schooldi-btn.wxss';
.hd{
    width: 100%;
    height: 175rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 60rpx;
    /* justify-items: center; */
}
.hd .hd-desc{
    display: inline-block;
    color: #333333;
    font-size: 40rpx;
}
.hd .rules{
    font-size: 30rpx;
    color: #d1d1d1;
    padding-top: 30rpx;
}

.ft{
    margin-top: 102rpx;
}
.ft .cancel{
    width: 710rpx;
    height: 100rpx;
    background-color: #fff;
    right: 0;
    color: #fff;
    left: 0;
    top: 0;
    bottom: 0;
    border: 1px solid #4d4d4d;
    color: #000;
    margin-bottom: 20rpx;
}
